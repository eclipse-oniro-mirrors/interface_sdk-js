+export function getJsDocNodeCheckConfigItem(tagName: string[], message: string, type: DiagnosticCategory,
+  tagNameShouldExisted: boolean, checkValidCallback?: CheckValidCallbackInterface): JsDocNodeCheckConfigItem {
+  return {
