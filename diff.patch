+          .replace(/\\/g, '/');
+      });
+      globalObject.projectConfig.allModulesPaths.push(...modulePaths);
+    }
+  });
+  const defaultSdkConfigs: SdkConfig[] = [
+    {
+      'apiPath': systemModulePathArray,
+      'prefix': '@ohos'
+    }, {
+      'apiPath': systemModulePathArray,
+      'prefix': '@system'
+    }, {
+      'apiPath': systemModulePathArray,
+      'prefix': '@arkts'
+    }
+  ];
+  const externalApiPathStr = globalObject.projectConfig.externalApiPaths || '';
