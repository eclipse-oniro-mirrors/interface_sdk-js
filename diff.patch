+function mkDir(path_: string): void {
+  const parent: string = path.join(path_, '..');
+  if (!(fs.existsSync(parent) && !fs.statSync(parent).isFile())) {
+    mkDir(parent);
+  }
+  fs.mkdirSync(path_);
