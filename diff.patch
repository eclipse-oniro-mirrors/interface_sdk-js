+declare namespace roomLocation {
+  /**
+   * Interface for room location result
+   * @interface RoomLocateResponse
+   * @syscap SystemCapability.MultimodalAwareness.RoomLocation
+   * @systemapi
+   * @since 20
+   * @arkts 1.1&1.2
