+
+import type { Callback } from './@ohos.base';
+
+/**
+ * This module provides the capability to subscribe to report the room location result.
+ * @namespace roomLocation
+ * @syscap SystemCapability.MultimodalAwareness.RoomLocation
+ * @systemapi
+ * @since 20
+ * @arkts 1.1&1.2
