     *
     * @param fn condition
     *
     * @returns the last element that satisfies fn
     */
    public findLast(fn: (val: number, index: number, array: Uint16Array) => boolean): number {
        for (let i = this.lengthInt - 1; i >= 0; i--) {
            let val = this.getUnsafe(i).toDouble()
            if (fn(val, i, this)) {
                return val
            }
        }
